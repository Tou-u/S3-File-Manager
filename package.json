{
  "name": "image-upload",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "proxy": "wrangler dev node_modules/cfw-bindings-wrangler-bridge/worker/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "d1:mig:local": "wrangler d1 migrations apply s3manager --local",
    "d1:mig:prod": "wrangler d1 migrations apply s3manager"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "4.1.0",
    "@sveltejs/kit": "2.5.0",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@types/eslint": "8.56.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "autoprefixer": "10.4.17",
    "cfw-bindings-wrangler-bridge": "0.10.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "postcss": "8.4.35",
    "postcss-load-config": "5.0.3",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.1.2",
    "prettier-plugin-tailwindcss": "0.5.11",
    "svelte": "4.2.10",
    "svelte-check": "3.6.4",
    "tailwindcss": "3.4.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.1.1",
    "wrangler": "3.28.1"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "3.511.0",
    "@aws-sdk/s3-request-presigner": "3.511.0",
    "@lucia-auth/adapter-sqlite": "3.0.0",
    "bits-ui": "^0.18.0",
    "clsx": "2.1.0",
    "filepond": "4.30.6",
    "filepond-plugin-file-validate-type": "1.2.8",
    "filepond-plugin-image-exif-orientation": "1.0.11",
    "filepond-plugin-image-preview": "4.6.12",
    "lucia": "3.0.1",
    "lucide-svelte": "0.323.0",
    "mode-watcher": "0.2.0",
    "tailwind-merge": "2.2.1",
    "tailwind-variants": "0.1.20",
    "vaul-svelte": "^0.2.3"
  }
}
